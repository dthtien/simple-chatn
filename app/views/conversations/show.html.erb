<div class="card">
  <div class="row">
    <div class="col s3 l3">
      
    </div>
    <div class="col s9 l9">
      <div class="conversation">
        <div class="receiver center">
          <p class="chip blue white-text"><%= @conversation.receiver.name %></p>
        </div>

        <div class="list-messages">
        <%= render @conversation.messages %> 
        <div class="form-message">
          <%= render 'messages/form' %>
        </div>
        </div>

      </div>
    </div>
  </div>
</div>